public class Tile {
  private int _x;
  private int _y;
  private boolean _shown;
  
  public Tile(x, y) {
    _x = x;
    _y = y;
    _shown = false;
  }
  
  public static int getX() {
    return _x;
  }
  
  public static int getY() {
    return _y;
  }
  
  public static boolean isShown() {
    return _shown;
  }
    
}